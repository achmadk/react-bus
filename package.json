{
  "name": "react-bus",
  "description": "A global event emitter for react.",
  "version": "1.0.1",
  "author": "Ren√©e Kooi <renee@kooi.me>",
  "bugs": {
    "url": "https://github.com/goto-bus-stop/react-bus/issues"
  },
  "dependencies": {
    "mitt": "^1.1.3",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "ascjs": "^2.4.0",
    "buble": "^0.18.0",
    "react": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "tape": "^4.8.0"
  },
  "homepage": "https://github.com/goto-bus-stop/react-bus#readme",
  "keywords": [
    "bus",
    "event-emitter",
    "eventemitter",
    "pubsub",
    "react"
  ],
  "license": "MIT",
  "main": "index.cjs.js",
  "module": "index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goto-bus-stop/react-bus.git"
  },
  "scripts": {
    "build": "npm run build:es && npm run build:cjs",
    "build:cjs": "ascjs index.js | buble --no modules --objectAssign Object.assign -o index.cjs.js",
    "build:es": "buble --no modules --objectAssign Object.assign index.js -o index.es.js",
    "prepare": "npm run build",
    "test": "node test"
  }
}
